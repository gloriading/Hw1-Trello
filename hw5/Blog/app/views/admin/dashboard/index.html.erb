<div class="container">


<h2>Admin Dashboard</h2>

<h4>All users:</h4>
<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Email</th>
    </tr>
  </thead>
  <tbody>

<% @users.each do |user| %>
    <tr class="table-success">
      <td><%= user.first_name %></td>
      <td><%= user.last_name %></td>
      <td><%= user.email %></td>
    </tr>
<% end %>

  </tbody>
</table>


</div>
